	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.24",
		"drizzle-kit": "^0.31.1",
		"vitest": "~3.1.2",
		"@types/node": "^22.15.3"
	},
	"dependencies": {
		"drizzle-orm": "^0.44.2",
		"drizzle-seed": "^0.3.1"
	}
}
